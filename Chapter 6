,\n    {\n      \"column\": \"price\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 54.25525468804429,\n        \"min\": 7.99,\n        \"max\": 225.15,\n        \"num_unique_values\": 4629,\n        \"samples\": [\n          186.29,\n          49.12\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"units\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 5,\n        \"min\": 0,\n        \"max\": 41,\n        \"num_unique_values\": 36,\n        \"samples\": [\n          29,\n          19\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"revenue\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 297.3953604573635,\n        \"min\": 0.0,\n        \"max\": 2145.92,\n        \"num_unique_values\": 5182,\n        \"samples\": [\n          925.82,\n          461.51\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{},"execution_count":1}],"source":["import numpy as np\n","import pandas as pd\n","\n","np.random.seed(42)\n","\n","# Parameters\n","n_products = 60\n","n_days = 90\n","rows = n_products * n_days\n","\n","# Static product attributes\n","categories = np.random.choice(\n","    [\"Electronics\",\"Home\",\"Beauty\",\"Apparel\",\"Sports\"],\n","    size=n_products, p=[0.25,0.2,0.2,0.2,0.15]\n",")\n","base_price = np.round(np.random.uniform(10, 200, size=n_products), 2)\n","base_quality = np.round(np.random.uniform(0.4, 0.95, size=n_products), 2)\n","inventory_tightness = np.round(np.random.beta(2, 5, size=n_products), 2)\n","\n","product_frame = pd.DataFrame({\n","    \"product_id\": np.arange(1, n_products+1),\n","    \"category\": categories,\n","    \"base_price\": base_price,\n","    \"base_quality\": base_quality,\n","    \"inventory_tightness\": inventory_tightness\n","})\n","\n","# Day-level features\n","days = np.tile(np.arange(1, n_days+1), n_products)\n","product_ids = np.repeat(product_frame[\"product_id\"].values, n_days)\n","dow = ((days % 7) + 1)\n","dow_factor = 1.0 + 0.1*np.sin(dow/7 * 2*np.pi)\n","\n","# Marketing\n","ad_spend = np.random.gamma(shape=2.0, scale=20.0, size=rows)\n","promo_flag = np.random.binomial(1, p=0.18, size=rows)\n","\n","# Merge\n","df = pd.DataFrame({\n","    \"product_id\": product_ids, \"day\": days, \"dow\": dow,\n","    \"ad_spend\": ad_spend, \"promo\": promo_flag, \"dow_factor\": dow_factor\n","}).merge(product_frame, on=\"product_id\", how=\"left\")\n","\n","# Realized price\n","price = (\n","    df[\"base_price\"] * (1 - 0.20*df[\"promo\"]) * (1 + 0.05*np.random.randn(rows))\n",").clip(5, None)\n","\n","# Latent demand\n","true_eta = (\n","    1.2*df[\"base_quality\"]\n","    + 0.0009*df[\"ad_spend\"]\n","    + 0.25*df[\"promo\"]\n","    - 0.010*price\n","    - 0.35*df[\"inventory_tightness\"]\n",")\n","mu = np.exp(2.1 + true_eta)\n","units = np.random.poisson(lam=np.clip(mu, 0.1, 300))\n","revenue = price * units\n","\n","df[\"price\"] = np.round(price, 2)\n","df[\"units\"] = units\n","df[\"revenue\"] = np.round(revenue, 2)\n","\n","df.head()\n"]},{"cell_type":"markdown","source":["SECTION 2: Demand Forecasting with Machine Learning"],"metadata":{"id":"VeUrNrvpJpGF"}},{"cell_type":"code","source":["from sklearn.model_selection import train_test_split\n","from sklearn.preprocessing import OneHotEncoder, StandardScaler\n","from sklearn.compose import ColumnTransformer\n","from sklearn.pipeline import Pipeline\n","from sklearn.ensemble import RandomForestRegressor\n","from sklearn.metrics import r2_score, mean_absolute_error\n","\n","# Features and target\n","features = [\"category\",\"price\",\"ad_spend\",\"promo\",\"base_quality\",\"inventory_tightness\",\"dow_factor\"]\n","target = \"units\"\n","\n","X = df[features]\n","y = df[target]\n","\n","# Preprocessing\n","numeric = [\"price\",\"ad_spend\",\"promo\",\"base_quality\",\"inventory_tightness\",\"dow_factor\"]\n","categorical = [\"category\"]\n","\n","preprocess = ColumnTransformer([\n","    (\"cat\", OneHotEncoder(handle_unknown=\"ignore\"), categorical),\n","    (\"num\", StandardScaler(), numeric)\n","])\n","\n","# Train/test\n","X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.25, random_state=42)\n","\n","# Model\n","model = Pipeline([\n","    (\"prep\", preprocess),\n","    (\"rf\", RandomForestRegressor(n_estimators=200, random_state=42))\n","])\n","model.fit(X_train, y_train)\n","\n","pred = model.predict(X_test)\n","\n","print(\"R^2:\", round(r2_score(y_test, pred), 3))\n","print(\"MAE:\", round(mean_absolute_error(y_test, pred), 3))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1g4Nu1rDJtm2","executionInfo":{"status":"ok","timestamp":1756992715185,"user_tz":240,"elapsed":12769,"user":{"displayName":"Nick Rezaee","userId":"03052255344002966927"}},"outputId":"5a9734bf-7ab1-4017-c370-cf2405fe8111"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["R^2: 0.702\n","MAE: 2.318\n"]}]},{"cell_type":"markdown","source":["SECTION 3: Product Segmentation with K-means"],"metadata":{"id":"-9hw7wHLJvPv"}},{"cell_type":"code","source":["from sklearn.cluster import KMeans\n","import matplotlib.pyplot as plt\n","from sklearn.preprocessing import StandardScaler\n","\n","# Aggregate product-level features\n","agg = df.groupby(\"product_id\").agg({\n","    \"category\":\"first\",\n","    \"base_price\":\"first\",\n","    \"base_quality\":\"first\",\n","    \"inventory_tightness\":\"first\",\n","    \"units\":\"mean\",\n","    \"revenue\":\"mean\",\n","    \"promo\":\"mean\",\n","    \"ad_spend\":\"mean\"\n","}).reset_index()\n","\n","seg_features = [\"base_price\",\"base_quality\",\"inventory_tightness\",\"units\",\"revenue\",\"promo\",\"ad_spend\"]\n","Z = StandardScaler().fit_transform(agg[seg_features])\n","\n","kmeans = KMeans(n_clusters=4, n_init=\"auto\", random_state=42)\n","agg[\"segment\"] = kmeans.fit_predict(Z)\n","\n","print(agg.head())\n","\n","# Plot segments\n","plt.scatter(Z[:,0], Z[:,3], c=agg[\"segment\"], alpha=0.8)\n","plt.xlabel(\"Std base_price\")\n","plt.ylabel(\"Std units\")\n","plt.title(\"Figure A: Product Segments\")\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":717},"id":"sQwdhWtRJwz2","executionInfo":{"status":"ok","timestamp":1756992717108,"user_tz":240,"elapsed":1925,"user":{"displayName":"Nick Rezaee","userId":"03052255344002966927"}},"outputId":"6bcfd06f-cfef-440e-f9df-cb445ad900c3"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["   product_id     category  base_price  base_quality  inventory_tightness  \\\n","0           1         Home       83.85          0.84                 0.17   \n","1           2       Sports       61.56          0.89                 0.54   \n","2           3      Apparel      167.46          0.57                 0.06   \n","3           4       Beauty       77.78          0.46                 0.24   \n","4           5  Electronics       63.38          0.53                 0.31   \n","\n","       units     revenue     promo   ad_spend  segment  \n","0  10.700000  837.739333  0.233333  40.918139        3  \n","1  12.655556  725.967556  0.255556  38.377766        1  \n","2   3.488889  554.683778  0.177778  39.370922        0  \n","3   7.044444  512.359778  0.233333  44.772957        0  \n","4   7.588889  459.387222  0.155556  45.783317        0  \n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["SECTION 4: Dynamic Pricing Policy and A/B Evaluation"],"metadata":{"id":"YnBY8fTfJ0VW"}},{"cell_type":"code","source":["# Rolling mean units\n","df[\"units_ma7\"] = df.groupby(\"product_id\")[\"units\"].transform(lambda s: s.rolling(7, min_periods=1).mean())\n","q25, q75 = df[\"units_ma7\"].quantile([0.25, 0.75])\n","\n","# Simple rule\n","def price_policy(row):\n","    p = row[\"price\"]\n","    if row[\"units_ma7\"] >= q75: p *= 1.05\n","    elif row[\"units_ma7\"] <= q25: p *= 0.93\n","    return max(5.0, p)\n","\n","df_eval = df.copy()\n","df_eval[\"price_new\"] = df_eval.apply(price_policy, axis=1)\n","\n","# Predict counterfactual units\n","X_counter = df_eval[features].copy()\n","X_counter[\"price\"] = df_eval[\"price_new\"]\n","df_eval[\"units_pred_new\"] = model.predict(X_counter)\n","df_eval[\"revenue_new\"] = df_eval[\"price_new\"] * df_eval[\"units_pred_new\"]\n","\n","# Product-level summary\n","ab = df_eval.groupby(\"product_id\").agg(\n","    old_rev=(\"revenue\",\"mean\"),\n","    new_rev=(\"revenue_new\",\"mean\")\n",").reset_index()\n","ab[\"lift_pct\"] = 100*(ab[\"new_rev\"] - ab[\"old_rev\"]) / (ab[\"old_rev\"]+1e-9)\n","\n","print(ab.describe()[[\"old_rev\",\"new_rev\",\"lift_pct\"]])\n","\n","# Distribution\n","plt.hist(ab[\"lift_pct\"], bins=25)\n","plt.title(\"Figure B: Revenue Lift Distribution\")\n","plt.xlabel(\"Lift percent\")\n","plt.ylabel(\"Count of products\")\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":642},"id":"mHS8bkjZJ05-","executionInfo":{"status":"ok","timestamp":1756992718817,"user_tz":240,"elapsed":1707,"user":{"displayName":"Nick Rezaee","userId":"03052255344002966927"}},"outputId":"240d3025-2022-4b76-950b-b9d542d97ea8"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["          old_rev     new_rev   lift_pct\n","count   60.000000   60.000000  60.000000\n","mean   556.114676  575.731017   3.933325\n","std    159.831203  159.638650   4.342800\n","min    137.969667  148.474655  -4.488988\n","25%    481.397778  497.707966   1.122407\n","50%    554.592000  583.769357   3.786153\n","75%    681.184861  688.331441   6.142146\n","max    870.100000  905.672062  15.736275\n"]},{"output_type":"display_data","data":{"text